{
  "name": "agda",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_top_level"
      }
    },
    "_top_level": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "pragma"
        },
        {
          "type": "SYMBOL",
          "name": "module_decl"
        }
      ]
    },
    "name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_name_part"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "_"
              },
              {
                "type": "SYMBOL",
                "name": "_name_part"
              }
            ]
          }
        }
      ]
    },
    "qualified_name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_name_part"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "_name_part"
              }
            ]
          }
        }
      ]
    },
    "anonymous_name": {
      "type": "STRING",
      "value": "_"
    },
    "_name_part": {
      "type": "PATTERN",
      "value": "[^\\s\\_\\;\\.\\\"\\(\\)\\{\\}\\@]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "--"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "{-"
              },
              {
                "type": "PATTERN",
                "value": "[^#].*\\r?\\n?"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^-]"
                    },
                    {
                      "type": "PATTERN",
                      "value": "-[^}]"
                    }
                  ]
                }
              },
              {
                "type": "PATTERN",
                "value": "-}\\r?\\n"
              }
            ]
          }
        ]
      }
    },
    "pragma": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{-#"
        },
        {
          "type": "SYMBOL",
          "name": "pragma_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_pragma_argument"
          }
        },
        {
          "type": "STRING",
          "value": "#-}"
        }
      ]
    },
    "pragma_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "BUILTIN"
        },
        {
          "type": "STRING",
          "value": "CATCHALL"
        },
        {
          "type": "STRING",
          "value": "COMPILE"
        },
        {
          "type": "STRING",
          "value": "FOREIGN"
        },
        {
          "type": "STRING",
          "value": "NO_POSITIVITY_CHECK"
        },
        {
          "type": "STRING",
          "value": "NO_TERMINATION_CHECK"
        },
        {
          "type": "STRING",
          "value": "TERMINATING"
        },
        {
          "type": "STRING",
          "value": "NON_TERMINATING"
        },
        {
          "type": "STRING",
          "value": "POLARITY"
        },
        {
          "type": "STRING",
          "value": "STATIC"
        },
        {
          "type": "PATTERN",
          "value": "\\S+"
        }
      ]
    },
    "_pragma_argument": {
      "type": "PATTERN",
      "value": "\\S+"
    },
    "module_decl": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "qualified_name"
            },
            {
              "type": "SYMBOL",
              "name": "anonymous_name"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "where"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s|\\\\n"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": []
}