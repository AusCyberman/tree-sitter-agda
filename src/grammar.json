{
  "name": "agda",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_declaration"
          }
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": "\\\".*\\\""
    },
    "int": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[0-9]"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "[0-9]"
            }
          }
        ]
      }
    },
    "name": {
      "type": "PATTERN",
      "value": "[^\\s\\;\\.\\\"\\(\\)\\{\\}\\@]+"
    },
    "binding_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "_"
        },
        {
          "type": "SYMBOL",
          "name": "name"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "--"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "{-"
              },
              {
                "type": "PATTERN",
                "value": "[^#]"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[^-]"
                    },
                    {
                      "type": "PATTERN",
                      "value": "-[^}]"
                    }
                  ]
                }
              },
              {
                "type": "PATTERN",
                "value": "-}"
              }
            ]
          }
        ]
      }
    },
    "pragma": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{-#"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^#]"
                },
                {
                  "type": "PATTERN",
                  "value": "#[^-]"
                },
                {
                  "type": "PATTERN",
                  "value": "#\\-[^}]"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "#-}"
          }
        ]
      }
    },
    "_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "infix"
        },
        {
          "type": "SYMBOL",
          "name": "pragma"
        }
      ]
    },
    "infix": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "infix"
            },
            {
              "type": "STRING",
              "value": "infixl"
            },
            {
              "type": "STRING",
              "value": "infixr"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "int"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "binding_name"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s|\\\\n"
    }
  ],
  "conflicts": [
    [
      "ReferenceError"
    ]
  ],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_newline"
    },
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    }
  ],
  "inline": []
}